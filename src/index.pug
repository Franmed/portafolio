<!DOCTYPE html>
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		meta(http-equiv="X-UA-Compatible", content="ie=edge")
		link(rel="stylesheet", href="css/materialize.min.css")
		link(href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
		link(rel="stylesheet", href="css/index.css")
		title Francisco Medina
	body
		header
			//TOP of the Page
			div(class="Top__Header__Container grey darken-4")
				div(class="row Top__Header")
					div(class="Top__Contact__Phone col s6 hide-on-small-and-down")
						span Phone: (+58)424-403.66.59
					div(class="Top__Contact__Social col s6 right")
					div(class="Top__Contact__Social__Items right")
						a(href="#")
							span(class="Social__Item fa fa-facebook-square" aria-hidden="true")
						a(href="#")
							span(class="Social__Item fa fa-twitter-square" aria-hidden="true")
						a(href="#")
							span(class="Social__Item fa fa-linkedin-square" aria-hidden="true")
			
			//NAVBAR PRINCIPAL
			div(class="Principal__Nav__Container")
				nav(class="row Principal__Nav grey lighten-4")
					div(class="nav-wrapper Principal__Nav__UlContainer")
						a(href="#" class="left brand-logo Principal__Nav__Logo") {FM} 
							
						<a href="#" data-activates="Principal__Menu__Toggle" class="right button-collapse Principal__Menu__Toggle"><i class="material-icons">menu</i></a>
						ul(id="nav-mobile" class="right hide-on-med-and-down")
							li
								a(href="#" class="Principal__Nav__Item") Item 1
							li
								a(href="#" class="Principal__Nav__Item") Item 2
							li
								a(href="#" class="Principal__Nav__Item") Item 3
							li
								a(href="#" class="Principal__Nav__Item") Item 4

						ul(id="Principal__Menu__Toggle" class="side-nav")
							li
								a(href="#" class="Principal__Nav__Item") Item 1
							li
								a(href="#" class="Principal__Nav__Item") Item 2
							li
								a(href="#" class="Principal__Nav__Item") Item 3
							li
								a(href="#" class="Principal__Nav__Item") Item 4
			
			//IMAGEN PRINCIPAL AL ENTRAR
			section
				div(class="Principal__Section__Container")
					div(class="Principal__Section")
						h1(class="Principal__Section__Title") FRANCISCO MEDINA
						h4(class="Principal__Section__Subtitle") Web Developer
						i(class="material-icons Principal__Section__Arrow") expand_more
			
			//SECCION DEL ABOUT
			section
				div(class="container Second__Section__About__Container")
